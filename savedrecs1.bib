@article{ ISI:000224993500008,
Author = {King, KA and Vaughan, JL},
Title = {{Influence of paper color and a monetary incentive on response rate}},
Journal = {{PSYCHOLOGICAL REPORTS}},
Year = {{2004}},
Volume = {{95}},
Number = {{2}},
Pages = {{432-434}},
Month = {{OCT}},
Abstract = {{This study examined whether survey response rate differed based on the
   color of the paper the survey was printed on (blue vs green) and
   presence of a monetary incentive. A4-page survey on eating disorders was
   mailed to Division 1A and lAA college head athletic trainers (N=223)
   with half of the surveys on blue paper and half on green paper. Half of
   the athletic trainers (n=111) received a \$1.00 monetary incentive, and
   half (n=112) received no monetary incentive. A total of 166 (71\%)
   athletic trainers returned completed surveys. Response rates did not
   differ based on survey color but did differ based on presence of a
   monetary incentive. Athletic trainers who received a monetary incentive
   were significantly more likely than those who did not to return
   completed surveys (86\% vs 63\%, respectively).}},
DOI = {{10.2466/pr0.95.2.432-434}},
